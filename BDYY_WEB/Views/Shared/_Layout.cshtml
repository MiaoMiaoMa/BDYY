<!DOCTYPE html>
<html ng-app="mainApp">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge;chrome=1" />

    <title>@ViewBag.Title</title>
    
    @Styles.Render("~/Content/Main.css")
    @Styles.Render("~/Content/Search.css")
    @Scripts.Render("~/Scripts/jquery-1.8.2.min.js")
    @Scripts.Render("~/Scripts/angular.js")
    @Scripts.Render("~/Scripts/Views/tools.js")
    @Scripts.Render("~/Scripts/default.js")
    
    <link href="~/Content/jquery-ui.css") rel="stylesheet" />
    <script src="~/Scripts/jquery-ui-1.8.24.min.js"></script>
    <script src="~/Scripts/jquery-ui-timepicker-addon.js"></script>
    <script src="~/Scripts/jquery-ui-datepicker.js"></script>
</head>
<body>
<div style="text-align: center;">
        <table align="center" width="922" border="0" cellpadding="0" cellspacing="0">
            <tr>
                <td>
                    <table class="header_bg" border="0" cellpadding="0" cellspacing="0">
                        <tr>
                            <td class="header1"></td>
                        </tr>
                    </table>
                    <div class="mb_10"></div>
                    <div class="float_l">
                        <div class="news mb_10">
                            <div class="news_header"><span class="news_title">快捷菜单</span></div>

                            <div class="clear"></div>
                            <div class="loginstatus">
                                <ul>
                                    <li><a href="../Account/AccountInfo">基本信息</a></li>
                                    <li><a href="../Account/ApplyFor">首次申请用药</a></li>
                                    <li><a href="../Account/ApplyAgain">续领申请用药</a></li>
                                    <li><a href="../Account/StatusCheck">状态查询</a></li>
                                    <li><a href="../Account/ChangePWD">修改密码</a></li>
                                    <li><a href="../Home/Logout">注销退出</a></li>
                                </ul>
                            </div>
                            <div class="clear"></div>
                            <div class="news_footer"></div>
                        </div>

                        <div class="clear"></div>
                        <div class="news">
                            <div class="news_header"><span class="news_title">最新消息</span></div>
                            <div class="news_content">
                                <ul>
                                    <li>111</li>
                                </ul>
                            </div>
                            <div class="news_footer"></div>
                        </div>
                    </div>
    @RenderBody()
           <div class="clear"></div>
                </td>
            </tr>
            <tr>
                <td style="padding-top: 15px;">
                    <div class="footer pt_10">
                        <div class="footer_bottom">
                            Copyrights © 2015 kmn.org.cn All Rights Reserved
                            <br />
                            凯美纳后续免费用药项目版权所有
                        </div>
                    </div>
                </td>
            </tr>
        </table>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
